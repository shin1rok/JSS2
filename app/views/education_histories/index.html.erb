<p id="notice"><%= notice %></p>

<h1>Education Histories</h1>

<table>
  <thead>
    <tr>
      <th>Profile</th>
      <th>School name</th>
      <th>Entrance date</th>
      <th>Graduation date</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @education_histories.each do |education_history| %>
      <tr>
        <td><%= education_history.profile %></td>
        <td><%= education_history.school_name %></td>
        <td><%= education_history.entrance_date %></td>
        <td><%= education_history.graduation_date %></td>
        <td><%= education_history.status %></td>
        <td><%= link_to 'Show', education_history %></td>
        <td><%= link_to 'Edit', edit_education_history_path(education_history) %></td>
        <td><%= link_to 'Destroy', education_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Education History', new_education_history_path %>
